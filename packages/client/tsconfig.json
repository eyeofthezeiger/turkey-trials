{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./dist",
    "lib": ["DOM", "DOM.Iterable", "ES6"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
